{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/setup.vbs",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/x-vbscript"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=\"setup.vbs\""
        }
      ]
    }
  ]
}
