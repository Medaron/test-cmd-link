{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/setup.vbs",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/x-vbscript"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=\"setup.vbs\""
        }
      ]
    }
  ]
}
